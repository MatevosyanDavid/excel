{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-console": "off"
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base/legacy"
  ]
}
